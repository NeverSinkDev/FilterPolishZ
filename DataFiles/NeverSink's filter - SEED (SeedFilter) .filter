#------------------------------------

Show # %d2
	Width <= 1
	Height <= 4
	ItemLevel <= 4
	Rarity Normal
	SetFontSize 36
	SetBorderColor 0 0 0                 # BORDERCOLOR:	 Neutral T1.5
	SetBackgroundColor 0 0 0 185         # BACKGROUND:	 Neutral T3

Show # %d2
	Width <= 2
	Height <= 2
	ItemLevel <= 4
	Rarity Normal
	SetFontSize 36
	SetBorderColor 0 0 0                 # BORDERCOLOR:	 Neutral T1.5
	SetBackgroundColor 0 0 0 185         # BACKGROUND:	 Neutral T3

